{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"noEmit": true
	},
	"include": [
		"src/**/*.ts",
		"src/**/*.tsx",
		"tests/**/*.ts",
		"tests/**/*.tsx",
		"*.js",
		"*.ts",
		".eslintrc.js",
		"jest.config.js",
		"jest.setup.js",
		"next.config.js",
		"postcss.config.js",
		"middleware.ts"
	],
	"exclude": ["node_modules", ".next", "build", "dist"]
}
